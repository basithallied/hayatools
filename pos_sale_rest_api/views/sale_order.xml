<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_order_inherit_b2b" model="ir.ui.view">
        <field name="name">sale.order.view.b2b</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
           <xpath expr="//label[@for='commitment_date']" position="after">
                <field name="delivery_priority_id"/>
                <field name="delivery_description"/>
           </xpath>
        </field>
    </record>
</odoo>
